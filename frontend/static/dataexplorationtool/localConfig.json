{
    "proxyUrl": "/proxy/?url=",
    "geoStoreUrl": "/rest/geostore",
    "printUrl": "https://demo.geo-solutions.it/geoserver/pdf/info.json",
    "themePrefix": "dataexplorationtool",
    "translationsPath": [
        "/static/dataexplorationtool/translations",
        "/static/dataexplorationtool/MapStore2/web/client/translations"
    ],
    "geocoderStyle": {
        "iconUrl": "/static/dataexplorationtool/img/marker.svg",
        "shadowUrl": "/static/dataexplorationtool/img/marker-shadow.png",
        "iconSize": [
            25,
            41
        ],
        "iconAnchor": [
            12,
            41
        ],
        "popupAnchor": [
            1,
            -34
        ],
        "shadowSize": [
            41,
            41
        ],
        "stroke": {
            "color": "#1477c9",
            "width": 2,
            "opacity": 1,
            "lineDash": [
                4,
                6
            ]
        },
        "fill": {
            "color": "rgba(18, 18, 18, 0.1)"
        }
    },
    "initialState": {
        "defaultState": {
            "maptype": {
                "mapType": "openlayers"
            }
        }
    },
    "plugins": {
        "dataexplorationtool": [
            {
                "name": "Map",
                "cfg": {
                    "mapType": "openlayers"
                }
            },
            "BrandNavbar",
            {
                "name": "OmniBar",
                "cfg": {
                    "containerPosition": "header",
                    "className": "navbar shadow navbar-home"
                }
            },
            {
                "name": "DataExplorer",
                "cfg": {
                    "containerPosition": "columns"
                }
            },
            "Toolbar",
            "Expander",
            "Undo",
            "Redo",
            "ZoomAll",
            {
                "name": "FullScreen",
                "override": {
                    "Toolbar": {
                        "alwaysVisible": false
                    }
                }
            },
            {
                "name": "ZoomIn",
                "override": {
                    "Toolbar": {
                        "alwaysVisible": true
                    }
                }
            },
            {
                "name": "ZoomOut",
                "override": {
                    "Toolbar": {
                        "alwaysVisible": true
                    }
                }
            },
            "DataExplorerButton"
        ]
    }
}